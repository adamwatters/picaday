<% provide(:title, @user.name) %>
<div class="center-block">
  <section class="user_info">
    <h1>
      <%= @user.name %>
    </h1>
    <% if current_user?(@user) %>
      <span><%= link_to "Log out", logout_path, method: "delete", class:'button'%></span>
    <% end %>
  </section>
  <% if @user.sequences.any? %>
    <div class="sequences">
      <%= sequences_react_component(@sequences, showUserInfo: false, showDeleteLinks: true) %>
    </div>
  <% end %>
</div>