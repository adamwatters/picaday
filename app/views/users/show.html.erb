<% provide(:title, @user.name) %>
<div class="center-block">
  <section class="user_info">
    <h1>
      <%= @user.name %>
    </h1>
    <span><%= link_to "Log out", logout_path, method: "delete" %></span>
  </section>
  <% if @user.sequences.any? %>
    <div class="sequences">
      <%= sequences_react_component(@sequences) %>
    </div>
  <% end %>
</div>